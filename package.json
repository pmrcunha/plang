{
  "name": "plang",
  "version": "0.1.7",
  "description": "A minimal, JS only localization library",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "clear": "rm -rf dist && mkdir dist",
    "build": "yarn clear && babel index.js --out-file dist/index.js",
    "bump-patch": "yarn version --new-version patch --no-git-tag-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmrcunha/plang.git"
  },
  "keywords": [
    "localization",
    "intl",
    "i18n",
    "internationalization"
  ],
  "author": "Paulo Cunha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pmrcunha/plang/issues"
  },
  "homepage": "https://github.com/pmrcunha/plang#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.10.0",
    "eslint-config-pmrcunha": "^0.2.8",
    "jest": "^21.2.1"
  },
  "dependencies": {
    "react": "^16.0.0"
  }
}
